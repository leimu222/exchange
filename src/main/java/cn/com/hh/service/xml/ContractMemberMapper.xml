<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.common.api.mapper.ContractMemberMapper">

    <resultMap id="ContractMemberResult" type="ContractMember" >
            <result property="memberId" column="member_id" />
            <result property="ifProxy" column="if_proxy" />
            <result property="proxyId" column="proxy_id" />
            <result property="invitationCode" column="invitation_code" />
            <result property="contractRebateRate" column="contract_rebate_rate" />
            <result property="spotRebateRate" column="spot_rebate_rate" />
            <result property="nodeId" column="node_id" />
            <result property="status" column="status" />
            <result property="version" column="version" />
            <result property="createTime" column="create_time" />
            <result property="sequence" column="sequence" />
    </resultMap>

    <sql id="selectContractMemberVo">
        select   member_id,  if_proxy,  proxy_id,  invitation_code,  contract_rebate_rate,  spot_rebate_rate,  node_id,  status,  version,  create_time,  sequence 
        from contract_member
    </sql>
    <!-- 查询列表 -->
    <select id="selectContractMemberList" parameterType="ContractMember" resultMap="ContractMemberResult">
        <include refid="selectContractMemberVo"/>
        <where>
                    <if test = "memberId != null">
                        and member_id  = #{memberId}
                    </if>
                    <if test = "ifProxy != null">
                        and if_proxy  = #{ifProxy}
                    </if>
                    <if test = "proxyId != null">
                        and proxy_id  = #{proxyId}
                    </if>
                    <if test = "invitationCode != null and invitationCode != ''">
                        and invitation_code  = #{invitationCode}
                    </if>
                    <if test = "contractRebateRate != null">
                        and contract_rebate_rate  = #{contractRebateRate}
                    </if>
                    <if test = "spotRebateRate != null">
                        and spot_rebate_rate  = #{spotRebateRate}
                    </if>
                    <if test = "nodeId != null">
                        and node_id  = #{nodeId}
                    </if>
                    <if test = "status != null">
                        and status  = #{status}
                    </if>
                    <if test = "version != null">
                        and version  = #{version}
                    </if>
                    <if test = "createTime != null">
                        and create_time  = #{createTime}
                    </if>
                    <if test = "sequence != null">
                        and sequence  = #{sequence}
                    </if>
        </where>
    </select>
    <!-- 根据ID查询实体类 -->
    <select id="selectContractMemberById" parameterType="Long" resultMap="ContractMemberResult">
        <include refid="selectContractMemberVo"/>
        where  member_id  = #{memberId} 
    </select>
    <!-- 插入实体类 -->
    <insert id="insertContractMember" parameterType="ContractMember" useGeneratedKeys="true" >
        insert into contract_member
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test = "memberId != null">
                member_id,
            </if>
            <if test = "ifProxy != null">
                if_proxy,
            </if>
            <if test = "proxyId != null">
                proxy_id,
            </if>
            <if test = "invitationCode != null">
                invitation_code,
            </if>
            <if test = "contractRebateRate != null">
                contract_rebate_rate,
            </if>
            <if test = "spotRebateRate != null">
                spot_rebate_rate,
            </if>
            <if test = "nodeId != null">
                node_id,
            </if>
            <if test = "status != null">
                status,
            </if>
            <if test = "version != null">
                version,
            </if>
            <if test = "createTime != null">
                create_time,
            </if>
            <if test = "sequence != null">
                sequence,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test = "memberId != null">
                #{memberId},
            </if>
            <if test = "ifProxy != null">
                #{ifProxy},
            </if>
            <if test = "proxyId != null">
                #{proxyId},
            </if>
            <if test = "invitationCode != null">
                #{invitationCode},
            </if>
            <if test = "contractRebateRate != null">
                #{contractRebateRate},
            </if>
            <if test = "spotRebateRate != null">
                #{spotRebateRate},
            </if>
            <if test = "nodeId != null">
                #{nodeId},
            </if>
            <if test = "status != null">
                #{status},
            </if>
            <if test = "version != null">
                #{version},
            </if>
            <if test = "createTime != null">
                #{createTime},
            </if>
            <if test = "sequence != null">
                #{sequence},
            </if>
        </trim>
    </insert>
    <!-- 更新实体类 -->
    <update id="updateContractMember" parameterType="ContractMember">
        update contract_member
        <trim prefix="SET" suffixOverrides=",">
            <if test = "ifProxy != null">
            if_proxy  = #{ifProxy},
            </if>
            <if test = "proxyId != null">
            proxy_id  = #{proxyId},
            </if>
            <if test = "invitationCode != null">
            invitation_code  = #{invitationCode},
            </if>
            <if test = "contractRebateRate != null">
            contract_rebate_rate  = #{contractRebateRate},
            </if>
            <if test = "spotRebateRate != null">
            spot_rebate_rate  = #{spotRebateRate},
            </if>
            <if test = "nodeId != null">
            node_id  = #{nodeId},
            </if>
            <if test = "status != null">
            status  = #{status},
            </if>
            <if test = "version != null">
            version  = #{version},
            </if>
            <if test = "createTime != null">
            create_time  = #{createTime},
            </if>
            <if test = "sequence != null">
            sequence  = #{sequence},
            </if>
        </trim>
        where  member_id  = #{memberId} 
    </update>
    <delete id="deleteContractMemberById" parameterType="Long">
        delete from contract_member where  member_id  = #{memberId} 
    </delete>

    <delete id="deleteContractMemberByIds" parameterType="Long">
        delete from contract_member where member_id in
        <foreach item="memberId" collection="array" open="(" separator="," close=")">
            #{memberId}
        </foreach>
    </delete>
</mapper>